{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "fig",
      "camelcase": "Fig",
      "title": "Fig",
      "scope": "source.fig",
      "file-types": [
        "fig"
      ],
      "injection-regex": "^fig$",
      "class-name": "TreeSitterFig",
      "highlights": ["queries/highlights.scm"]
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Parser for the Fig programming language",
    "authors": [
      {
        "name": "Luca Mezzavilla",
        "email": "lucamezza4@gmail.com",
        "url": ""
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-fig",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}