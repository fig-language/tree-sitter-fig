{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "nyx",
      "camelcase": "Nyx",
      "title": "Nyx",
      "scope": "source.nyx",
      "file-types": [
        "nyx"
      ],
      "injection-regex": "^nyx$",
      "class-name": "TreeSitterNyx"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Parser for the Nyx programming language",
    "authors": [
      {
        "name": "Luca Mezzavilla",
        "email": "lucamezza4@gmail.com",
        "url": ""
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-nyx",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}